
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Create</h1>
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label  class="control-label">Kullanıcı Seçiniz</label>
               <select class="form-control" name="" asp-items="ViewBag.User"></select>
                
            </div>
            <div class="form-group">
                
                <label class="control-label">Rol Seçiniz</label>
                <select class="form-control" name="" asp-items="ViewBag.Role"></select>
               
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

